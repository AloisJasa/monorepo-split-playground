{
    "name": "aloisjasa/monorepo-split-playground",
    "authors": [
        {
            "name": "aloisjasa",
            "email": "aloisjasa@gmail.com"
        }
    ],
    "require": {
        "php": ">= 8.1",
        "nette/utils": "^3.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.7.14",
        "phpstan/phpstan-doctrine": "^1.3.9",
        "phpstan/phpstan-nette": "^1.0.0",
        "phpstan/phpstan-phpunit": "^1.1",
        "phpunit/phpunit": "^9.5 || ^10",
        "symplify/monorepo-builder": "11.1.17"
    },
    "autoload": {
        "psr-4": {
            "AloisJasa\\MonorepoSplitPlaygroundSub1\\": "packages/monorepo-split-playground-sub1/src/",
            "AloisJasa\\MonorepoSplitPlaygroundSub2\\": "packages/monorepo-split-playground-sub2/src/",
            "AloisJasa\\MonorepoTools\\": "packages/monorepo-tools/src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/AloisJasa/monorepo-split-playground-sub1"
        },
        {
            "type": "vcs",
            "url": "https://github.com/AloisJasa/monorepo-split-playground-sub2"
        }
    ],
    "replace": {
        "aloisjasa/monorepo-split-playground-sub1": "self.version",
        "aloisjasa/monorepo-split-playground-sub2": "self.version"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
